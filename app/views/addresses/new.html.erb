<hr>
<h4>Complete The Fields To Add A New Address</h4>
<%= form_for([@user, @address], url: user_addresses_path) do |f| %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.label :nickname %>
  <%= f.text_field :nickname %>

<%= f.submit 'Submit' %>
<% end  %>
